{
  "agents": {
    "defaults": {
      "model": "google/gemini-3-flash-preview"
    }
  },
  "gateway": {
    "mode": "local",
    "bind": "lan"
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "dmPolicy": "open",
      "allowFrom": ["*"],
      "groupPolicy": "open",
      "groups": {
        "*": {
          "requireMention": false
        }
      },
      "mediaMaxMb": 20
    }
  }
}
