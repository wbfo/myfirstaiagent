{
  "agents": {
    "defaults": {
      "model": "google/gemini-3-flash-preview",
      "subagents": {
        "maxConcurrent": 4,
        "maxSpawnDepth": 1,
        "model": "google/gemini-3-flash-preview"
      },
      "skills": [
        "honeybadger-cloudrun-ops",
        "honeybadger-telegram-diagnostics",
        "honeybadger-config-secrets-audit",
        "honeybadger-observability",
        "honeybadger-skill-factory",
        "skill-creator"
      ]
    },
    "list": [
      {
        "id": "honeybadger",
        "default": true,
        "name": "HoneyBadger Orchestrator",
        "model": "google/gemini-3-flash-preview",
        "skills": [
          "honeybadger-cloudrun-ops",
          "honeybadger-telegram-diagnostics",
          "honeybadger-config-secrets-audit",
          "honeybadger-observability",
          "honeybadger-skill-factory",
          "skill-creator"
        ],
        "subagents": {
          "allowAgents": [
            "ops-coordinator",
            "quality-gate",
            "architect",
            "researcher",
            "deal-closer",
            "market-advisory"
          ]
        }
      },
      {
        "id": "ops-coordinator",
        "name": "Ops Coordinator",
        "model": {
          "primary": "google/gemini-3-flash-preview",
          "fallbacks": ["google/gemini-2.5-flash"]
        },
        "skills": ["honeybadger-observability", "honeybadger-cloudrun-ops"]
      },
      {
        "id": "quality-gate",
        "name": "Quality Gate",
        "model": {
          "primary": "google/gemini-3-flash-preview",
          "fallbacks": ["google/gemini-2.5-flash"]
        },
        "skills": ["honeybadger-config-secrets-audit", "honeybadger-observability"]
      },
      {
        "id": "architect",
        "name": "Architect",
        "model": {
          "primary": "google/gemini-3-pro-preview",
          "fallbacks": ["google/gemini-3-flash-preview"]
        },
        "skills": [
          "honeybadger-cloudrun-ops",
          "honeybadger-config-secrets-audit",
          "honeybadger-observability"
        ]
      },
      {
        "id": "researcher",
        "name": "Researcher",
        "model": {
          "primary": "google/gemini-3-pro-preview",
          "fallbacks": ["google/gemini-3-flash-preview"]
        },
        "skills": ["honeybadger-observability", "honeybadger-skill-factory"]
      },
      {
        "id": "deal-closer",
        "name": "Deal Closer",
        "model": {
          "primary": "google/gemini-3-flash-preview",
          "fallbacks": ["google/gemini-2.5-flash"]
        },
        "skills": ["honeybadger-observability", "honeybadger-skill-factory"]
      },
      {
        "id": "market-advisory",
        "name": "Market Advisory",
        "model": {
          "primary": "google/gemini-3-flash-preview",
          "fallbacks": ["google/gemini-2.5-flash"]
        },
        "skills": ["honeybadger-observability", "honeybadger-skill-factory"]
      }
    ]
  },
  "gateway": {
    "mode": "local",
    "bind": "lan"
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "dmPolicy": "open",
      "allowFrom": ["*"],
      "groupPolicy": "open",
      "groups": {
        "*": {
          "requireMention": false
        }
      },
      "mediaMaxMb": 20
    }
  }
}
