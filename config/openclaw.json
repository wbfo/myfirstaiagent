{
  "meta": {
    "lastTouchedVersion": "2026.2.22",
    "lastTouchedAt": "2026-02-24T20:10:00Z"
  },
  "env": {
    "vars": {
      "OLLAMA_API_KEY": "ollama"
    }
  },
  "agents": {
    "list": [
      {
        "id": "main",
        "name": "Main Assistant",
        "default": false,
        "workspace": "~/.openclaw/workspace",
        "model": {
          "primary": "google/gemini-3-flash-preview",
          "fallbacks": [
            "google/gemini-2.5-flash"
          ]
        },
        "subagents": {
          "allowAgents": [
            "*"
          ]
        }
      },
      {
        "id": "honeybadger",
        "name": "HoneyBadger Orchestrator",
        "default": true,
        "workspace": "~/.openclaw/workspace",
        "model": {
          "primary": "google/gemini-3-flash-preview",
          "fallbacks": [
            "google/gemini-2.5-flash"
          ]
        },
        "subagents": {
          "allowAgents": [
            "ops-coordinator",
            "quality-gate",
            "architect",
            "researcher",
            "deal-closer",
            "market-advisory"
          ]
        }
      },
      {
        "id": "architect",
        "name": "Architect",
        "workspace": "~/.openclaw/workspace",
        "model": {
          "primary": "google/gemini-2.5-pro",
          "fallbacks": [
            "google/gemini-3-flash-preview"
          ]
        }
      },
      {
        "id": "researcher",
        "name": "Researcher",
        "workspace": "~/.openclaw/workspace",
        "model": {
          "primary": "google/gemini-2.5-pro",
          "fallbacks": [
            "google/gemini-3-flash-preview"
          ]
        }
      },
      {
        "id": "deal-closer",
        "name": "Deal Closer",
        "workspace": "~/.openclaw/workspace",
        "model": {
          "primary": "google/gemini-2.5-flash",
          "fallbacks": [
            "google/gemini-3-flash-preview"
          ]
        }
      },
      {
        "id": "market-advisory",
        "name": "Market Advisory",
        "workspace": "~/.openclaw/workspace",
        "model": {
          "primary": "openai/gpt-4o-mini",
          "fallbacks": [
            "google/gemini-2.5-flash"
          ]
        }
      },
      {
        "id": "ops-coordinator",
        "name": "Ops Coordinator",
        "workspace": "~/.openclaw/workspace",
        "model": {
          "primary": "google/gemini-3-flash-preview",
          "fallbacks": [
            "google/gemini-2.5-flash"
          ]
        }
      },
      {
        "id": "quality-gate",
        "name": "Quality Gate",
        "workspace": "~/.openclaw/workspace",
        "model": {
          "primary": "google/gemini-3-flash-preview",
          "fallbacks": [
            "google/gemini-2.5-flash"
          ]
        }
      }
    ],
    "defaults": {
      "workspace": "~/.openclaw/workspace",
      "contextTokens": 65536,
      "memorySearch": {
        "enabled": true,
        "extraPaths": [
          "IDEAS.md",
          "SOUL.md",
          "IDENTITY.md",
          "USER.md"
        ],
        "provider": "gemini"
      },
      "compaction": {
        "mode": "safeguard"
      },
      "maxConcurrent": 4,
      "subagents": {
        "maxConcurrent": 8,
        "maxSpawnDepth": 2,
        "model": "google/gemini-2.5-flash"
      },
      "sandbox": {
        "mode": "off"
      }
    }
  },
  "tools": {
    "deny": [
      "browser"
    ],
    "exec": {
      "pathPrepend": [
        "./bin"
      ]
    }
  },
  "messages": {
    "ackReactionScope": "group-mentions"
  },
  "commands": {
    "native": "auto",
    "nativeSkills": "auto",
    "restart": true,
    "ownerDisplay": "raw"
  },
  "channels": {
    "telegram": {
      "dmPolicy": "pairing",
      "groupPolicy": "allowlist",
      "voice": {
        "enabled": true,
        "voice": "en-US-AndrewNeural",
        "mode": "always"
      },
      "streaming": "off"
    }
  },
  "gateway": {
    "mode": "local",
    "trustedProxies": [],
    "http": {
      "endpoints": {
        "responses": {
          "files": {
            "pdf": {
              "maxPages": 10,
              "minTextChars": 100
            }
          }
        }
      }
    }
  },
  "costOptStack": true,
  "plugins": {
    "entries": {
      "telegram": {
        "enabled": true
      }
    }
  }
}
