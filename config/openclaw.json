    "defaults": {
      "workspace": "~/.openclaw/workspace",
      "contextTokens": 65536,
      "memorySearch": {
        "enabled": true,
        "extraPaths": ["IDEAS.md", "SOUL.md", "IDENTITY.md", "USER.md"],
        "provider": "gemini"
      },
      "compaction": {
        "mode": "safeguard"
      },
      "maxConcurrent": 4,
      "subagents": {
        "maxConcurrent": 8,
        "maxSpawnDepth": 2,
        "model": "google/gemini-2.5-flash"
      },
      "sandbox": {
        "mode": "off"
      }
    }
