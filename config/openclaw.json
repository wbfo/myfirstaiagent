      "compaction": {
        "mode": "safeguard"
      },
      "maxConcurrent": 4,
      "subagents": {
        "maxConcurrent": 3,
        "maxSpawnDepth": 3,
        "model": {
          "primary": "ollama/qwen2.5:14b",
          "fallbacks": ["ollama/llama3.1:8b"]
        }
      },
      "sandbox": {
        "mode": "off"
