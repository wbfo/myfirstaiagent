        "tools": {
          "profile": "full"
        },
        "subagents": {
          "allowAgents": ["*"]
        }
      },
      {
        "id": "code-specialist",
        "name": "Code Specialist",
        "workspace": "/Users/wbfoclaw/OPENopenclaw",
        "tools": {
          "profile": "coding"
        },
        "subagents": {
          "allowAgents": ["*"]
        }
      }
    ],
    "defaults": {
      "model": {
        "primary": "google/gemini-2.0-flash",
        "fallbacks": [
          "ollama/qwen2.5:14b",
          "ollama/llama3.1:8b",
          "openai/gpt-4o",
          "anthropic/claude-3-5-sonnet-20240620"
        ]
      },
      "imageModel": {
        "primary": "google/gemini-2.0-flash",
        "fallbacks": [
          "ollama/llama3.2-vision",
          "ollama/qwen2.5vl",
          "ollama/llava",
          "openai/gpt-4o",
          "anthropic/claude-3-5-sonnet-20240620"
        ]
      },
      "workspace": "/Users/wbfoclaw/.openclaw/workspace",
      "contextTokens": 65536,
      "memorySearch": {
        "enabled": true,
        "extraPaths": ["IDEAS.md", "SOUL.md", "IDENTITY.md", "USER.md"],
        "provider": "gemini"
