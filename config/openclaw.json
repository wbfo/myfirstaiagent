  "tools": {
    "deny": ["browser"],
    "exec": {
      "pathPrepend": ["./bin"]
    }
  },
  "messages": {
    "ackReactionScope": "group-mentions"
  },
  "commands": {
    "native": "auto",
    "nativeSkills": "auto",
    "restart": true,
    "ownerDisplay": "raw"
  },
  "channels": {
    "telegram": {
      "dmPolicy": "pairing",
      "groupPolicy": "allowlist",
      "voice": {
        "enabled": true,
        "voice": "en-US-AndrewNeural",
        "mode": "always"
      },
      "streaming": "off"
    }
  },
  "gateway": {
    "mode": "local",
    "trustedProxies": [],
    "http": {
      "endpoints": {
        "responses": {
          "files": {
            "pdf": {
              "maxPages": 10,
              "minTextChars": 100
            }
          }
        }
      }
    }
  },
  "costOptStack": true,
  "plugins": {
    "entries": {
      "telegram": {
        "enabled": true
      }
    }
  }
